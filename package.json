{
  "name": "nopyprompt",
	"displayName": "NoPyPrompt",
	"description": "Removes >>> and ... from copy/pasted Python shell code and comments out the outputs. And vice versa.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:nopyprompt.togglePrompts"
	],
	"main": "./extension.js",
	"contributes": {
		"keybindings": {
			"command": "nopyprompt.togglePrompts",
			"key": "ctrl+shift+,",
			"mac": "cmd+shift+,"
		},
		"commands": [{
            "command": "nopyprompt.togglePrompts",
            "title": "NoPyPrompt: Toggle Python Prompts >>>"
		}
	]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }

}
